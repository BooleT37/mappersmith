{
  "compilerOptions": {
    "outDir": "./built",
    "incremental": true,
    "strict": true,
    "noImplicitAny": true,
    "declaration": true,
    "target": "es5"
  },
  "include": [
    "typings/**/*",
    "spec/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "spec/**/*",
  ]
}
